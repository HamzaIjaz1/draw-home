{
  "name": "@draw-house/planner",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx --max-warnings=0 src .eslintrc.js vite.config.ts vitest-reporter.ts --ignore-pattern src/migrateProject/bundle.js",
    "lint:fix": "npm run lint -- --fix",
    "ts:noWatch": "tsc --noEmit -p tsconfig.json",
    "ts": "npm run ts:noWatch -- --watch",
    "_vite": "nodemon --delay 0.5 -e js -w ../../.env -w ../common/dist -w ../ui/dist -x \"dotenv -e ../../.env -- vite --force\"",
    "dev": "concurrently -r \"npm run ts -- --preserveWatchOutput\" \"npm run _vite\"",
    "build": "npm run ts:noWatch && dotenv -e ../../.env -- vite build",
    "preview": "vite preview",
    "strapi:migrate-project:build": "npx esbuild src/migrateProject/source.js --tsconfig=tsconfig.json --bundle --platform=node --outfile=src/migrateProject/bundle.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:update": "vitest run -u",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@arthurka/json-serializer": "1.1.0",
