    "@gltf-transform/functions": "4.2.1",
    "@highlight-run/react": "19.0.21",
    "@mui/material": "5.15.20",
    "@react-three/drei": "9.115.0",
    "@react-three/fiber": "8.15.16",
    "@reactour/tour": "3.8.0",
    "@stripe/react-stripe-js": "5.4.0",
    "@stripe/stripe-js": "8.5.2",
    "assert": "2.1.0",
    "color": "5.0.0",
    "cytoscape": "3.30.0",
    "data-structure-typed": "1.52.0",
    "debounce": "1.2.1",
    "decimal.js": "10.4.3",
    "file-saver": "2.0.5",
    "filenamify": "6.0.0",
    "framer-motion": "11.5.4",
    "fuzzysort": "3.1.0",
    "highlight.run": "9.18.21",
    "lzbase62": "2.0.0",
    "path-browserify": "1.0.1",
