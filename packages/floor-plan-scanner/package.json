{
  "name": "@draw-house/floor-plan-scanner",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx --max-warnings=0 src .eslintrc.js",
    "lint:fix": "npm run lint -- --fix",
    "ts:noWatch": "tsc --noEmit -p tsconfig.json",
    "ts": "npm run ts:noWatch -- --watch",
    "_express": "nodemon --delay 0.5 -e js,ts -w ../../.env -w ../common/dist -w src -x \"dotenv -e ../../.env -- ts-node --transpile-only src/index.ts\"",
    "dev": "concurrently -r \"npm run ts -- --preserveWatchOutput\" \"npm run _express\"",
    "build": "tsc -p tsconfig.json",
    "start": "dotenv -e ../../.env -- node dist/index.js"
  },
  "dependencies": {
    "@arthurka/ts-utils": "3.17.1",
    "@draw-house/common": "1.0.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv-cli": "7.4.2",
    "express": "5.1.0",
    "three": "0.161.0",
    "zod": "3.25.74"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.8",
    "@types/cors": "2.8.18",
    "@types/express": "5.0.2",
    "@types/node": "20.11.17",
    "@types/three": "0.161.2",
    "concurrently": "8.2.2",
    "nodemon": "3.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.5.2"
  }
}
